        "–ü—Ä–∏–≤–µ—Ç! üëã –Ø –±–æ—Ç –¥–ª—è –≥–∞–¥–∞–Ω–∏—è –æ—Ç —Ç–∞—Ä–æ–ª–æ–≥–∞ –ê–Ω–∞—Å—Ç–∞—Å–∏–∏ –ß–∞–π–∫–∏. –ò–Ω—Å—Ç–∞–≥—Ä–∞–º - @chaika.taro. –≠—Ç–æ—Ç –º–µ—Ç–æ–¥ –≥–∞–¥–∞–Ω–∏—è –æ—Å–Ω–æ–≤–∞–Ω –Ω–∞ –∫–∏—Ç–∞–π—Å–∫–æ–π –ö–Ω–∏–≥–µ –ü–µ—Ä–µ–º–µ–Ω, –∏–∑–≤–µ—Å—Ç–Ω–æ–π —Ç–∞–∫–∂–µ –∫–∞–∫ –ò-–¶–∑–∏–Ω. –î–ª—è –≥–∞–¥–∞–Ω–∏—è –∑–∞–¥–∞–π –∫–æ–Ω–∫—Ä–µ—Ç–Ω—ã–π –≤–æ–ø—Ä–æ—Å –∏ —Å–ª–µ–¥—É–π –∏–Ω—Å—Ç—Ä—É–∫—Ü–∏—è–º. –¢–µ–±–µ –≤—ã–ø–∞–¥–µ—Ç –≥–µ–∫—Å–∞–≥—Ä–∞–º–º–∞, –∏ —Ç—ã –ø–æ–ª—É—á–∏—à—å —Ç–æ–ª–∫–æ–≤–∞–Ω–∏–µ."
        "–¢—ã —Å–º–æ–∂–µ—à—å –ø–æ–ª—É—á–∏—Ç—å –º—É–¥—Ä—ã–π —Å–æ–≤–µ—Ç –¥–ª—è —Å–≤–æ–µ–π —Å–∏—Ç—É–∞—Ü–∏–∏! üßò‚Äç‚ôÇÔ∏è\n\n"
        "–ó–∞–¥–∞–π –≤–æ–ø—Ä–æ—Å, –Ω–∞ –∫–æ—Ç–æ—Ä—ã–π —Ö–æ—á–µ—à—å –ø–æ–ª—É—á–∏—Ç—å –æ—Ç–≤–µ—Ç. –ù–∞–ø—Ä–∏–º–µ—Ä:\n"
        "- –ö–∞–∫ –º–Ω–µ –ø–æ—Å—Ç—É–ø–∏—Ç—å –≤ —Å–ª–æ–∂–∏–≤—à–µ–π—Å—è —Å–∏—Ç—É–∞—Ü–∏–∏?\n"
        "- –°—Ç–æ–∏—Ç –ª–∏ –ø—Ä–∏–Ω–∏–º–∞—Ç—å –ø—Ä–µ–¥–ª–æ–∂–µ–Ω–∏–µ?"
        "–ù–∞–ø–∏—à–∏ —Å–≤–æ–π –≤–æ–ø—Ä–æ—Å –ø—Ä—è–º–æ –≤ –æ–∫–Ω–æ —Å–æ–±—â–µ–Ω–∏–π –∏ –æ—Ç–ø—Ä–∞–≤—å –µ–≥–æ"
    )

async def ask_question(update: Update, context: ContextTypes.DEFAULT_TYPE) -> None:
    """–ü–æ–ª—É—á–µ–Ω–∏–µ –≤–æ–ø—Ä–æ—Å–∞ –∏ –∑–∞–ø—É—Å–∫ –≥–∞–¥–∞–Ω–∏—è"""
    await update.message.reply_text(
        "–û—Ç–ª–∏—á–Ω–æ! –¢–µ–ø–µ—Ä—å –¥–∞–≤–∞–π –Ω–∞–π–¥–µ–º –æ—Ç–≤–µ—Ç –Ω–∞ —Ç–≤–æ–π –≤–æ–ø—Ä–æ—Å. –ö–æ–≥–¥–∞ –±—É–¥–µ—à—å –≥–æ—Ç–æ–≤, –Ω–∞–∂–º–∏ –Ω–∞ –∫–Ω–æ–ø–∫—É ¬´–û—Ç–ø—Ä–∞–≤–∏—Ç—å –≤–æ–ø—Ä–æ—Å¬ª. üé≤",
        reply_markup=ReplyKeyboardMarkup([['–û—Ç–ø—Ä–∞–≤–∏—Ç—å –≤–æ–ø—Ä–æ—Å']], one_time_keyboard=True, resize_keyboard=True)
    )

async def ask_question(update: Update, context: ContextTypes.DEFAULT_TYPE) -> None:
    """–û–±—Ä–∞–±–æ—Ç–∫–∞ –≤–æ–ø—Ä–æ—Å–æ–≤ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–π"""
    await update.message.reply_text("–¢–≤–æ–π –≤–æ–ø—Ä–æ—Å –ø—Ä–∏–Ω—è—Ç! –ü–æ–¥–æ–∂–¥–∏ —Å–µ–∫—É–Ω–¥—É... –ù–µ –≤–æ—Å–ø—Ä–∏–Ω–∏–º–∞–π –æ—Ç–≤–µ—Ç –±—É–∫–≤–∞–ª—å–Ω–æ. –¢—ã –º–æ–∂–µ—à—å —É–≤–∏–¥–µ—Ç—å —Å–æ–≤–µ—Ç—ã, –ø—Ä–µ–¥—É–ø—Ä–µ–∂–¥–µ–Ω–∏—è –∏–ª–∏ —Ñ–∏–ª–æ—Å–æ—Ñ—Å–∫–∏–µ —Ä–∞–∑–º—ã—à–ª–µ–Ω–∏—è –æ –∂–∏–∑–Ω–∏.")
    await hexagram(update, context)

async def hexagram(update: Update, context: ContextTypes.DEFAULT_TYPE) -> None:
    """–ì–µ–Ω–µ—Ä–∞—Ü–∏—è –≥–µ–∫—Å–∞–≥—Ä–∞–º–º—ã –∏ –æ—Ç–ø—Ä–∞–≤–∫–∞ —Ç–æ–ª–∫–æ–≤–∞–Ω–∏—è"""
    # –ì–µ–Ω–µ—Ä–∏—Ä—É–µ–º —Å–ª—É—á–∞–π–Ω—ã–π –Ω–æ–º–µ—Ä –≥–µ–∫—Å–∞–≥—Ä–∞–º–º—ã
    hexagram_number = random.randint(1, 64)
    # –ü–æ–ª—É—á–∞–µ–º —Ç–æ–ª–∫–æ–≤–∞–Ω–∏–µ –≥–µ–∫—Å–∞–≥—Ä–∞–º–º—ã
    interpretation = HEXAGRAMS[hexagram_number]
    
    await update.message.reply_text(
        f"–û—Ç–≤–µ—Ç –æ—Ç –∫–Ω–∏–≥–∏ –ø–µ—Ä–µ–º–µ–Ω: {hexagram_number}\n{interpretation}"
    )
def main() -> None:
    """–ó–∞–ø—É—Å–∫ –±–æ—Ç–∞"""
    application = Application.builder().token("8010384717:AAHssrQX8rPB_RZNH_ls8nXwvoTqbbyoVzE").build()

    application.add_handler(CommandHandler("start", start))
    application.add_handler(MessageHandler(filters.TEXT & ~filters.COMMAND, ask_question))
    application.add_handler(MessageHandler(filters.Regex('^–û—Ç–ø—Ä–∞–≤–∏—Ç—å –≤–æ–ø—Ä–æ—Å$'), hexagram))

    application.run_polling()

if __name__ == '__main__':
    main()